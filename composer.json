{
  "name": "lotgd/module-race",
  "description": "Prompts the user with a selection for a race at a new day.",
  "type": "lotgd-module",
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Basilius Sauter",
      "email": "basilius.sauter@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LotGD\\Module\\Race\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/lotgd/core.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/lotgd/module-new-day.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/lotgd/module-village.git"
    },
    {
      "type": "composer",
      "url": "http://code.lot.gd"
    }
  ],
  "require": {
    "lotgd/core": "^0.3.1-alpha",
    "lotgd/module-village": "^0.3.1-alpha",
    "lotgd/module-new-day": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1|^5.1",
    "phpunit/dbunit": "*"
  },
  "config": {
    "secure-http": false
  }
}
